<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SpringMVCPU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:app/pymes</jta-data-source>
    <class>entidades.Envio</class>
    <class>entidades.Pedido</class>
    <class>entidades.Producto</class>
    <class>entidades.EntradaProducto</class>
    <class>entidades.Proveedor</class>
    <class>entidades.Gerente</class>
    <class>entidades.Vendedor</class>
    <class>entidades.Contador</class>
    <class>entidades.Cliente</class>
    <class>entidades.PedidoProveedorxproducto</class>
    <class>entidades.Productoxpedido</class>
    <class>entidades.TipoUsuario</class>
    <class>entidades.Delivery</class>
    <class>entidades.PedidoProveedor</class>
    <class>entidades.Usuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
